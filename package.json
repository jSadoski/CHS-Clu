{
  "jest": {
    "testEnvironment": "node",
    "coveragePathIgnorePatterns": [
      "/node_modules/"
    ]
  },
  "name": "chs-clu",
  "version": "0.1.0",
  "description": "Clu is a general-use bot tailored for CoolHandSkywalker's Community Discord server.",
  "main": "index.js",
  "scripts": {
