  "devDependencies": {
    "concurrently": "^5.3.0",
    "dotenv": "^8.2.0",
    "eslint": "^7.15.0",
    "jest": "^26.6.3",
    "prettier": "^2.2.1",
    "sequelize-cli": "^6.2.0",
    "sqlite3": "^5.0.1",
    "supertest": "^6.0.1"
  },
  "dependencies": {
    "cors": "^2.8.5",
